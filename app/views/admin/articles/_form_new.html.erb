<%= semantic_form_for [:admin, @article],html: {:multipart => true} ,builder: Formtastic::FormBuilder do |f| %>
  <%= f.semantic_errors %>

  <%= f.inputs do %>
    <%= f.input :title %>
    <%= f.input :lat, input_html: {value: "#{@latitude}"}  %>
    <%= f.input :lng, input_html: {value: "#{@longitude}"}  %>
    <%= f.input :body, as: :ckeditor %>
    <% end %>
  <%= f.actions %>
<% end %>
